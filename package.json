{
  "name": "react-call",
  "version": "1.3.0",
  "description": "Call your React components",
  "repository": "desko27/react-call",
  "author": "Ismael Ramon <desko27@gmail.com> (https://desko.dev)",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "module": "./dist/main.js",
  "main": "./dist/main.cjs",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs",
      "types": "./dist/main.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "lint": "biome check .",
    "test": "exit 0",
    "check:types": "tsc --noEmit --p ./tsconfig.lib.json",
    "build": "npm run check:types && vite build",
    "bundlesize": "npm run build && bundlesize",
    "build:site": "npm run check:types && vite build -c vite.site.config.ts",
    "release": "np",
    "phoenix": "rm -rf node_modules && npm i",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@faker-js/faker": "^9.0.0",
    "@midudev/tailwind-animations": "^0.0.7",
    "@types/node": "^22.5.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "bundlesize": "^0.18.2",
    "clsx": "^2.1.1",
    "husky": "^9.1.5",
    "np": "^10.0.7",
    "postcss": "^8.4.42",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup-plugin-node-externals": "^7.1.3",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.0.3",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "peerDependencies": {
    "react": ">=18"
  }
}
